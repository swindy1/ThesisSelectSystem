    <column name="SpecialIdentity" use="true">specialIdentity</column>
    <column name="MaxMakeTopicNumber" use="true">maxMakeTopicNumber</column>
    <column name="TopicGroup" use="false">topicGroup</column>
  </table>

  <table name="userinfo" primary-key="Accounts">
    <column name="Accounts" use="true">account</column>
    <column name="Passwords" use="true">passwords</column>
    <column name="Salt" use="true">salt</column>
    <column name="Roles" use="true">roles</column>
  </table>
</root>