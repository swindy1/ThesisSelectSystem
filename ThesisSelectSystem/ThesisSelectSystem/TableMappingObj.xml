    <column name="Sex" use="true">sex</column>
    <column name="ShortTel" use="false">shortTel</column>
    <column name="LongTel" use="true">longTel</column>
    <column name="ClassId" use="true">classid</column>
    <column name="Sname" use="true">sname </column>
